{
  "name": "@idiosync/telescope",
  "version": "2.0.5",
  "description": "An application framework for react-native",
  "main": "lib/index.js",
  "types": "lib/index.d.ts", 
  "scripts": {
    "prepare" : "npm run build", 
    "prepublishOnly" : "npm test && npm run lint", 
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "test": "jest src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "contributors": [
    "James Trickey <james.trickeyd@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trickeyd/telescope.git"
  },
  "keywords": [
    "react",
    "react-native",
    "framework",
    "application",
    "MVC"
  ],
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "author": "James Trickey",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isnull": "^3.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.has": "^4.5.6",
    "@types/lodash.isarray": "^4.0.6",
    "@types/lodash.isboolean": "^3.0.6",
    "@types/lodash.isnull": "^3.0.6",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.isundefined": "^3.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.20",
    "eslint": "^5.16.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
